                      "aws:cdk:cloudformation:props": {
                        "name": "mysite.com.",
                        "type": "A",
                        "aliasTarget": {
                          "hostedZoneId": "Z2FDTNDATAQYW2",
                          "dnsName": {
                            "Fn::GetAtt": [
                              "StaticSiteCloudFrontDistributionCFDistributionA70E78CD",
                              "DomainName"
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
